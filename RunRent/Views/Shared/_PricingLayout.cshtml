@using System.Web.Optimization

<!DOCTYPE html>
<html lang="en">
<head>
    <title>RunRent</title>
    <meta charset="utf-8">
    <link rel="icon" href="~/Content/images/logo2.svg">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

    <link href="https://fonts.googleapis.com/css?family=Poppins:200,300,400,500,600,700,800&display=swap" rel="stylesheet">


    @Styles.Render("~/bundles/imagine/svg")
    @Styles.Render("~/bundles/open-iconic-bootstrap/css")
    @Styles.Render("~/bundles/animate/css")
    @Styles.Render("~/bundles/carousel/css")
    @Styles.Render("~/bundles/theme.default/css")
    @Styles.Render("~/bundles/magnific-popup/css")

    @Styles.Render("~/bundles/aos/css")
    @Styles.Render("~/bundles/ionicons/css")
    @Styles.Render("~/bundles/bootstrap-datepicker/css")
    @Styles.Render("~/bundles/jquery.timepicker/css")
    @Styles.Render("~/bundles/flaticon/css")
    @Styles.Render("~/bundles/icomoon/css")
    @Styles.Render("~/bundles/style/css")



</head>
<body>
    <nav class="navbar navbar-expand-lg navbar-dark ftco_navbar bg-dark ftco-navbar-light" id="ftco-navbar">
        @Html.Partial("_Header")
    </nav>

    <section class="hero-wrap hero-wrap-2 js-fullheight" style="background-image: url('/Content/images/bg_3.jpg');" data-stellar-background-ratio="0.5">
        @Html.Partial("_HeaderInfo")
    </section>

        <section class="ftco-section ftco-cart">
            @RenderBody();
        </section>

        <footer class="ftco-footer ftco-bg-dark ftco-section">
            @Html.Partial("_Footer")
        </footer>
        <div id="ftco-loader" class="show fullscreen">
            <svg class="circular" width="48px" height="48px">
                <circle class="path-bg" cx="24" cy="24" r="22" fill="none" stroke-width="4" stroke="#eeeeee" />
                <circle class="path" cx="24" cy="24" r="22" fill="none" stroke-width="4" stroke-miterlimit="10" stroke="#F96D00" />
            </svg>
        </div>
        <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBVWaKrjvy3MaE7SQ74_uJiULgl1JY0H2s&sensor=false"></script>
        <script type="module" src="https://unpkg.com/ionicons@5.5.2/dist/ionicons/ionicons.esm.js"></script>
        <script nomodule src="https://unpkg.com/ionicons@5.5.2/dist/ionicons/ionicons.js"></script>
        <script src="https://unpkg.com/aos@next/dist/aos.js"></script>


        @Scripts.Render("~/bundles/jquery/js")
        @Scripts.Render("~/bundles/jquery-migrate/js")
        @Scripts.Render("~/bundles/poper/js")
        @Scripts.Render("~/bundles/bootstrap/js")
        @Scripts.Render("~/bundles/jquery.easing/js")
        @Scripts.Render("~/bundles/jquery.waypoints/js")
        @Scripts.Render("~/bundles/jquery.stellar/js")
        @Scripts.Render("~/bundles/carousel/js")
        @Scripts.Render("~/bundles/jquery.magnific-popup/js")
        @Scripts.Render("~/bundles/jquery.animateNumber/js")
        @Scripts.Render("~/bundles/bootstrap-datepicker/js")
        @Scripts.Render("~/bundles/jquery.timepicker/js")
        @Scripts.Render("~/bundles/scrollax/js")
        @Scripts.Render("~/bundles/google-map/js")
        @Scripts.Render("~/bundles/main/js")
        @Scripts.Render("~/bundles/aos/js")
        @RenderSection("scripts", required: false)

</body>
</html>

